<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="HotkeySection" type="StarCatalog.HotkeySection, StarCatalog" />
  </configSections>
  <startup> 
      <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
  </startup>
  <appSettings>
    <add key="PluginsFolder" value="Plugins"/>  
  </appSettings>
  <HotkeySection>
    <Hotkeys>
      <Hotkey CommandName="SaveFile" Gesture="CTRL+S"/>
      <Hotkey CommandName="OpenFile" Gesture="CTRL+O"/>
      <Hotkey CommandName="ReloadPlugins" Gesture="CTRL+R"/>
      <Hotkey CommandName="CloseWindow" Gesture="ESC"/>
    </Hotkeys>
  </HotkeySection>
</configuration>