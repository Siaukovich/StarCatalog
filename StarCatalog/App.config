<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup> 
      <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
  </startup>
  <appSettings>
    <add key="PluginsFolder" value="Plugins"/>
    
    <!--Hotkeys-->
    <add key="PageView" value="Ctrl+W"/>
    <add key="SaveFile" value="Ctrl+s"/>
    <add key="OpenFile" value="Ctt"/>
    <add key="ReloadPlugins" value="Ctrl+X"/>
    <add key="Exit" value="Win+d"/>
    
  </appSettings>
</configuration>