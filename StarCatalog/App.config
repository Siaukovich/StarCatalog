<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup> 
      <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
  </startup>
  <appSettings>
    <!--Plugins Folder-->
    <add key="PluginsFolder" value="Plugins"/>
    
    <!--Hotkeys-->
    <add key="PageView" value="Ctrl+P"/>
    <add key="SaveFile" value="Ctrl+W"/>
    <add key="OpenFile" value="Ctrl+O"/>
    <add key="ReloadPlugins" value="Ctrl+R"/>
    <add key="Exit" value="Ctrl+W"/>
    
    <!--Menu Icons Folder-->
    <add key="IconPath" value="Icons"/>
    
    <!--Menu Icons Names-->
    <add key="OpenFileIcon" value="OpenFileIcon.png"/>
    <add key="SaveFileIcon" value="SaveFileIcon.png"/>
    <add key="ExitIcon" value="ExitIcon.png"/>
    <add key="ReloadPluginsIcon" value="ReloadPluginsIcon.png"/>
    
  </appSettings>
</configuration>